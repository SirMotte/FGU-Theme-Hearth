	<windowclass name="charsheetmini_main" merge="join">
		<script>
			function onInit()
				if super and super.onInit then
					super.onInit();
				end
				if hptemp then
					hptemp.setColor(ColorManager.COLOR_TEMP_HP);
				end
			end
		</script>
	</windowclass>
	<windowclass name="encounter_header" merge="join">
		<sheetdata>
			<stringfield name="name">
				<color merge="delete" />
				<selectioncolor merge="delete" />
			</stringfield>
		</sheetdata>
	</windowclass>
	<windowclass name="note_header" merge="join">
		<sheetdata>
			<stringfield name="name">
				<color merge="delete" />
				<selectioncolor merge="delete" />
			</stringfield>
		</sheetdata>
	</windowclass>
	<framedef name="acicon">
		<bitmap file="graphics/pngs/acicon_pfrpg.png" />
		<topleft rect="0,0,40,44" />
	</framedef>
	<template name="handle_record">
		<genericcontrol>
			<anchored height="25">
				<top offset="1" />
				<left offset="1" />
				<right offset="-1" />
			</anchored>
		</genericcontrol>
	</template>
</root>
